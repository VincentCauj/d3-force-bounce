{
  "name": "d3-force-bounce",
  "version": "0.4.1",
  "description": "An elastic collision force type for the d3-force simulation engine.",
  "keywords": [
    "d3",
    "d3-module",
    "d3-force",
    "bounce",
    "impact",
    "collision",
    "physics",
    "layout",
    "graph",
    "force",
    "simulation"
  ],
  "homepage": "https://github.com/vasturiano/d3-force-bounce",
  "license": "MIT",
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "main": "dist/d3-force-bounce.js",
  "module": "src/index",
  "jsnext:main": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-force-bounce.git"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c --banner \"$(preamble)\" -f umd -n d3 -o dist/d3-force-bounce.js -- src/index.js",
    "watch": "rollup -c -w -f umd -n d3 -o dist/d3-force-bounce.js -- src/index.js",
    "minify": "uglifyjs dist/d3-force-bounce.js -o dist/d3-force-bounce.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {
    "d3-quadtree": "^1.0.3"
  },
  "devDependencies": {
    "package-preamble": "~0.0.2",
    "rollup": "~0.41.6",
    "rollup-plugin-node-resolve": "^3.0",
    "rollup-watch": "^3.2",
    "uglify-js-es6": "^2.8.9"
  }
}
